{
    "version": "1.0",
    "message": "Welecome to the Moon!",
    "agent": {
        "child_exec": ".venv/Scripts/python",
        "child_args": [
            "agent.py"
        ]
    },
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "Default",
            "path": [
                "{PROJECT_DIR}/assets/resource/base"
            ]
        },
        {
            "name": "Bilibili",
            "path": [
                "{PROJECT_DIR}/assets/resource/base",
                "{PROJECT_DIR}/assets/resource/bilibili"
            ]
        }
    ],
    "task": [
        {
            "name": "MyTask2",
            "entry": "MyTask2"
        },
        {
            "name": "OCR",
            "entry": "Peek_OCR"
        },
        {
            "name": "TestBench01",
            "entry": "TestBench01"
        }
    ]
}